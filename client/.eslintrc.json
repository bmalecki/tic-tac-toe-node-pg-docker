{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jest/globals": true,    
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["jest"],  
  "rules": {
    "no-console": 1,
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}